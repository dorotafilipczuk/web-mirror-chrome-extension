{
  "manifest_version": 2,

  "name": "WM2 Chrome extension",
  "description": "This extension is for the Meaningful Consent in Digital Economy project, University of Southampton. Developed by Dorota Filipczuk and Richard Gomer.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": [
      "detection/addon.js",
      "detection/manualDeletion.js",
      "detection/setup.js"

    ]
  },

  "background": {
      "scripts": [ "detect.js"]
  },

  "permissions": [
    "activeTab",
    "<all_urls>",
    "management",
    "cookies",
    "history"

  ]
}
